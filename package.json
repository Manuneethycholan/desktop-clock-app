{
  "name": "desktop-clock-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://YOUR_GITHUB_USERNAME.github.io/desktop-clock-app",
  "dependencies": {
    "@types/node": "^16.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run optimize && npm run generate-manifest && react-scripts build && npm run post-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "generate-manifest": "node scripts/generateImageManifest.js",
    "optimize": "node scripts/optimizeImages.js",
    "post-build": "node scripts/postBuild.js",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "serve": "npx serve -s build -l 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "gh-pages": "^4.0.0",
    "source-map-explorer": "^2.5.3",
    "serve": "^14.2.0"
  }
}